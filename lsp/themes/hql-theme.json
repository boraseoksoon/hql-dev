{
  "name": "HQL Syntax Theme",
  "type": "dark",
  "colors": {},
  "tokenColors": [
    {
      "name": "Keywords",
      "scope": "keyword.control.hql",
      "settings": {
        "foreground": "#0000FF",
        "fontStyle": ""
      }
    },
    {
      "name": "Entity Types",
      "scope": [
        "entity.name.type.enum.hql",
        "entity.name.type.class.hql",
        "entity.name.type.hql"
      ],
      "settings": {
        "foreground": "#000000",
        "fontStyle": ""
      }
    },
    {
      "name": "Variables and Parameters",
      "scope": [
        "variable.other.hql",
        "variable.parameter.hql"
      ],
      "settings": {
        "foreground": "#000000",
        "fontStyle": ""
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function.hql",
        "entity.name.function.method.hql"
      ],
      "settings": {
        "foreground": "#000000",
        "fontStyle": ""
      }
    },
    {
      "name": "Strings",
      "scope": "string.quoted.double.hql",
      "settings": {
        "foreground": "#A31515",
        "fontStyle": ""
      }
    },
    {
      "name": "Numbers",
      "scope": "constant.numeric.hql",
      "settings": {
        "foreground": "#098658",
        "fontStyle": ""
      }
    },
    {
      "name": "Comments",
      "scope": [
        "comment.line.semicolon.hql",
        "comment.block.hql",
        "comment.line.double-slash.hql"
      ],
      "settings": {
        "foreground": "#008000",
        "fontStyle": ""
      }
    }
  ]
} 