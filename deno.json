{
  "name": "@boraseoksoon/hql",
  "version": "7.8.10",
  "exports": {
    ".": "./mod.ts",
    "./bundler": "./bundler.ts"
  },
  "license": "MIT",
  "tasks": {
    "embed-macros": "deno run -A tools/embed_macros.ts",
    "publish:prep": "deno task embed-macros",
    "publish": "deno task publish:prep && deno publish",
    "test:local:official": "deno run -A scripts/run-local-from-manifest.ts tests/manifest-official.json",
    "test:local:examples": "deno run -A scripts/run-local-from-manifest.ts tests/manifest-examples-curated.json",
    "test:jsr:official": "deno run -A scripts/run-jsr-7-8-7-from-manifest.ts tests/manifest-official.json",
    "test:jsr:examples": "deno run -A scripts/run-jsr-7-8-7-from-manifest.ts tests/manifest-examples-curated.json",
    "test:all": "deno task test:local:official && deno task test:jsr:official && deno task test:local:examples && deno task test:jsr:examples"
  }
}
